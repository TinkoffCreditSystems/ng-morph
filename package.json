{
    "name": "ng-morph",
    "version": "4.8.1",
    "private": true,
    "license": "Apache-2.0",
    "scripts": {
        "ng": "ng",
        "nx": "nx",
        "start": "nx run-many --target serve --all",
        "build": "nx run-many --target build --configuration production --verbose",
        "build:demo": "nx build demo --verbose",
        "postbuild:demo": "cp ./dist/apps/demo/index.html ./dist/apps/demo/404.html",
        "release": "nx run-many --target release --verbose",
        "pretest": "tsc --noEmit --strict --incremental false --tsBuildInfoFile null --project libs/ng-morph/tsconfig.spec.json",
        "test": "nx run-many --target test --verbose",
        "lint": "eslint .",
        "prettier": "prettier !package-lock.json !CHANGELOG.md . --ignore-path .gitignore",
        "affected:apps": "nx affected:apps",
        "affected:libs": "nx affected:libs",
        "affected:build": "nx affected:build",
        "affected:test": "nx affected:test",
        "stylelint": "stylelint '**/*.{less,css}' --config package.json",
        "affected:lint": "nx affected:lint",
        "affected:dep-graph": "nx affected:dep-graph",
        "affected": "nx affected",
        "format": "nx format:write",
        "format:write": "nx format:write",
        "format:check": "nx format:check",
        "update": "nx migrate latest",
        "workspace-generator": "nx workspace-generator",
        "dep-graph": "nx dep-graph",
        "help": "nx help"
    },
    "commitlint": {
        "extends": [
            "@taiga-ui/commitlint-config"
        ]
    },
    "lint-staged": {
        "*.less": [
            "stylelint --fix"
        ],
        "*.{js,ts,html}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{svg,yml,md,less,json}": [
            "prettier --write"
        ]
    },
    "browserslist": [
        "extends @taiga-ui/browserslist-config"
    ],
    "prettier": "@taiga-ui/prettier-config",
    "eslintConfig": {
        "extends": [
            "plugin:@taiga-ui/experience/all"
        ],
        "root": true
    },
    "stylelint": {
        "extends": [
            "@taiga-ui/stylelint-config"
        ],
        "ignoreFiles": [
            "**/dist/**",
            "**/coverage/**",
            "**/node_modules/**"
        ]
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "16.2.16",
        "@angular-devkit/core": "16.2.16",
        "@angular-devkit/schematics": "16.2.16",
        "@angular/animations": "16.2.12",
        "@angular/cdk": "16.2.14",
        "@angular/cli": "16.2.16",
        "@angular/common": "16.2.12",
        "@angular/compiler": "16.2.12",
        "@angular/compiler-cli": "16.2.12",
        "@angular/core": "16.2.12",
        "@angular/forms": "16.2.12",
        "@angular/platform-browser": "16.2.12",
        "@angular/platform-browser-dynamic": "16.2.12",
        "@angular/router": "16.2.12",
        "@nx/angular": "19.7.4",
        "@nx/jest": "19.7.4",
        "@nx/node": "19.7.4",
        "@nx/workspace": "19.7.4",
        "@release-it-plugins/workspaces": "4.2.0",
        "@release-it/bumper": "6.0.1",
        "@release-it/conventional-changelog": "8.0.2",
        "@schematics/angular": "16.2.16",
        "@taiga-ui/addon-doc": "3.94.0",
        "@taiga-ui/addon-mobile": "3.94.0",
        "@taiga-ui/browserslist-config": "0.200.0",
        "@taiga-ui/cdk": "3.94.0",
        "@taiga-ui/commitlint-config": "0.200.0",
        "@taiga-ui/core": "3.94.0",
        "@taiga-ui/cspell-config": "0.200.0",
        "@taiga-ui/eslint-plugin-experience": "0.200.0",
        "@taiga-ui/icons": "3.94.0",
        "@taiga-ui/kit": "3.94.0",
        "@taiga-ui/prettier-config": "0.200.0",
        "@taiga-ui/stylelint-config": "0.200.0",
        "@taiga-ui/tsconfig": "0.200.0",
        "@types/jest": "29.5.13",
        "@types/minimatch": "5.1.2",
        "@types/node": "22.5.5",
        "dotenv": "16.4.5",
        "highlight.js": "11.10.0",
        "jest": "29.7.0",
        "jest-environment-jsdom": "29.7.0",
        "jest-preset-angular": "14.2.4",
        "jsonc-parser": "3.3.1",
        "minimatch": "10.0.1",
        "multimatch": "5.0.0",
        "ngx-highlightjs": "10.0.0",
        "nx": "19.7.4",
        "release-it": "17.6.0",
        "rxjs": "7.8.1",
        "ts-jest": "29.2.5",
        "ts-morph": "23.0.0",
        "ts-node": "10.9.2",
        "tslib": "2.7.0",
        "typescript": "5.0.4"
    }
}
